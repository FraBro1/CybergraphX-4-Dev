SC	= sc
CPU	= 030
MATH	= 881
OPTIMIZE = OPTIMIZESCHEDULE OPTIMIZEPEEP OPTIMIZE

V3DDemo:	V3DDemo.c
		$(SC) V3DDemo.c MATH=$(MATH) CPU=$(CPU) $(OPTIMIZE) DATA=FAR LINK TO V3DDemo

V3DDemoLUT:	V3DDemo.c
		$(SC) V3DDemo.c MATH=$(MATH) CPU=$(CPU) $(OPTIMIZE) DATA=FAR DEF=LUTMODE LINK TO V3DDemoLUT

all:		V3DDemo V3DDemoLUT
